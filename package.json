  "jest": {
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules",
      "/home/vagrant/Bodylight.js-Composer/src"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
