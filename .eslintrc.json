{
  "env" : {
    "browser": true
  },
  "globals" : {
    "Module": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "no-console": "warn",
    "no-undef": "warn"
  }
}
