{
  "env" : {
    "browser": true,
    "node": false
  },
  "globals" : {
    "Module": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "no-console": "warn",
    "no-undef": 0,
    "no-path-concat": 0,
    "react/prop-types": 0,
    "no-new-func": 0
  }
}
